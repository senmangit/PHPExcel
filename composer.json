{
  "name": "senman/phpoffice",
  "description": "简单易用的phpoffice套件，可实现导入/导出excel、将word转为PDF,将pdf转为图片等等功能",
  "license": "MIT",
  "keywords": [
    "excel",
    "office",
    "php",
    "word",
    "export"
  ],
  "homepage": "https://www.monqin.com",
  "authors": [
    {
      "name": "senman",
      "email": "417131405@qq.com"
    }
  ],
  "support": {
    "issues": "https://github.com/senmangit/phpoffice"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "phpoffice/phpspreadsheet": "*"
  },
  "recommend": {
    "ext-zip": "*",
    "ext-gd": "*"
  },
  "autoload": {
    "psr-4": {
      "Excel\\": "src/Excel/",
      "Word\\": "src/Word/"
    }
  }
}
